vagrant up            			# create vms
vagrant ssh [name] 				# ssh into vm
vagrant halt          			# shut down vms
vagrant destroy [-f]     		# delete VMs
vagrant global-status --prune	# rm all broken vms
vagrant provision				# when provision script is edited
vagrant reload					# when config is edited
vagrant halt					# stop

kubectl get nodes -o wide		#get all machines
kubectl get all				#get all nodes etc.
curl -H "Host:[app]" 192.168.56.110	#curl front page
